#               Miller A. (30%)

text = input()
count_sentens = text.count('.')+text.count('!')+text.count('?')
count_words = (text.count(' ')+1)
print('Предложений:', count_sentens)
print('Слов:', count_words)
count_syllables = 0
symbol = set('ёуеэоаыяию')
for letter in text:
   if letter in symbol:
      count_syllables += 1
print('Слогов:', count_syllables)
